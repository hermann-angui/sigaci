                <th>Longitude</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for etablissement in etablissements %}
            <tr>
                <td>{{ etablissement.id }}</td>
                <td>{{ etablissement.primaryactivity }}</td>
                <td>{{ etablissement.activitystartdate }}</td>
                <td>{{ etablissement.secondaryactivity }}</td>
                <td>{{ etablissement.companyname }}</td>
                <td>{{ etablissement.companyacronym }}</td>
                <td>{{ etablissement.companypurpose }}</td>
                <td>{{ etablissement.companylegalstatus }}</td>
                <td>{{ etablissement.numregistremetier }}</td>
                <td>{{ etablissement.numrea }}</td>
                <td>{{ etablissement.numrccm }}</td>
                <td>{{ etablissement.companycapitalsocial }}</td>
