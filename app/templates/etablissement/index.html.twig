                <td>{{ etablissement.companyeffectapprentisfemme }}</td>
                <td>{{ etablissement.activitylocation }}</td>
                <td>{{ etablissement.activityLocation }}</td>
                <td>{{ etablissement.activitycountry }}</td>
                <td>{{ etablissement.activitycity }}</td>
                <td>{{ etablissement.activityquartier }}</td>
                <td>{{ etablissement.latitude }}</td>
                <td>{{ etablissement.longitude }}</td>
                <td>
                    <a href="{{ path('app_etablissement_show', {'id': etablissement.id}) }}">show</a>
                    <a href="{{ path('app_etablissement_edit', {'id': etablissement.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="40">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_etablissement_new') }}">Create new</a>
